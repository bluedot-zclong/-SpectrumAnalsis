package bluedot.spectrum.common;

import java.util.HashMap;
import java.util.Map;

import bluedot.spectrum.commons.vo.SpectrumFile;

public class Report {
	private SpectrumFile spectrum;
	private Map<String,String> map = new HashMap<>();
	public SpectrumFile getSpectrum() {
		return spectrum;
	}
	public void setSpectrum(SpectrumFile spectrum) {
		this.spectrum = spectrum;
	}
	public Map<String, String> getMap() {
		return map;
	}
	public void setMap(Map<String, String> map) {
		this.map = map;
	}
	public Report(SpectrumFile spectrum) {
		this.spectrum = spectrum;
	}
	
}
