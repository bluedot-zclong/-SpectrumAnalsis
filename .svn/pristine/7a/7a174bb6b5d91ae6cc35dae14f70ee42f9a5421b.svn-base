package bluedot.spectrum.commons.entity;

import java.util.Date;

/**
 * StandardSpectrum -> standard_spectrum
 * 2018-01-21
 */
public class StandardSpectrum {
    /**
     * 编号
     */
    private Long standardSpectrumId;

    /**
     * 标准光谱编号
     */
    private Integer standardSpectrumVersion;

    /**
     * 光谱名字
     */
    private String standardSpectrumName;

    /**
     * 光谱保存时间
     */
    private Date saveTime;

    /**
     * 光谱描述
     */
    private String spectrumDescription;

    /**
     * 光谱文件保存地址
     */
    private String spectrumFileUrl;

    /**
     * 被检测物产地
     */
    private String categoryOrigin;

    /**
     * 被检测物编号
     */
    private Long detectedId;

    /**
     * 用户编号
     */
    private Long userId;

    /**
     * 光谱图片路径
     */
    private String spectrumPictureUrl;

    /**
     * 光谱类型名称
     */
    private String spectrumTypeName;

    /**
     * 硬件名称
     */
    private String hardwareName;

    /**
     * 创建时间
     */
    private Date gmtCreate;

    /**
     * 最后修改时间
     */
    private Date gmtModified;

    /**
     * 光谱特征峰值
     */
    private String characteristicPeak;

    private static final long serialVersionUID = 1L;

    public Long getStandardSpectrumId() {
        return standardSpectrumId;
    }

    public void setStandardSpectrumId(Long standardSpectrumId) {
        this.standardSpectrumId = standardSpectrumId;
    }

    public Integer getStandardSpectrumVersion() {
        return standardSpectrumVersion;
    }

    public void setStandardSpectrumVersion(Integer standardSpectrumVersion) {
        this.standardSpectrumVersion = standardSpectrumVersion;
    }

    public String getStandardSpectrumName() {
        return standardSpectrumName;
    }

    public void setStandardSpectrumName(String standardSpectrumName) {
        this.standardSpectrumName = standardSpectrumName;
    }

    public Date getSaveTime() {
        return saveTime;
    }

    public void setSaveTime(Date saveTime) {
        this.saveTime = saveTime;
    }

    public String getSpectrumDescription() {
        return spectrumDescription;
    }

    public void setSpectrumDescription(String spectrumDescription) {
        this.spectrumDescription = spectrumDescription;
    }

    public String getSpectrumFileUrl() {
        return spectrumFileUrl;
    }

    public void setSpectrumFileUrl(String spectrumFileUrl) {
        this.spectrumFileUrl = spectrumFileUrl;
    }

    public String getCategoryOrigin() {
        return categoryOrigin;
    }

    public void setCategoryOrigin(String categoryOrigin) {
        this.categoryOrigin = categoryOrigin;
    }

    public Long getDetectedId() {
        return detectedId;
    }

    public void setDetectedId(Long detectedId) {
        this.detectedId = detectedId;
    }

    public Long getUserId() {
        return userId;
    }

    public void setUserId(Long userId) {
        this.userId = userId;
    }

    public String getSpectrumPictureUrl() {
        return spectrumPictureUrl;
    }

    public void setSpectrumPictureUrl(String spectrumPictureUrl) {
        this.spectrumPictureUrl = spectrumPictureUrl;
    }

    public String getSpectrumTypeName() {
        return spectrumTypeName;
    }

    public void setSpectrumTypeName(String spectrumTypeName) {
        this.spectrumTypeName = spectrumTypeName;
    }

    public String getHardwareName() {
        return hardwareName;
    }

    public void setHardwareName(String hardwareName) {
        this.hardwareName = hardwareName;
    }

    public Date getGmtCreate() {
        return gmtCreate;
    }

    public void setGmtCreate(Date gmtCreate) {
        this.gmtCreate = gmtCreate;
    }

    public Date getGmtModified() {
        return gmtModified;
    }

    public void setGmtModified(Date gmtModified) {
        this.gmtModified = gmtModified;
    }

    public String getCharacteristicPeak() {
        return characteristicPeak;
    }

    public void setCharacteristicPeak(String characteristicPeak) {
        this.characteristicPeak = characteristicPeak;
    }
}