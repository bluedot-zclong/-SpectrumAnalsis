
<#assign path="${basePath}">
<#macro bootstrapstatic bootstrap_css=["/assets/css/bootstrap.css"]
 bootstrap_js=["/assets/js/jquery.js","/assets/js/bootstrap.js"]>

	<#list bootstrap_css as css>
	<link rel="stylesheet" href="${path}${css}"/>
	</#list>
	<#list bootstrap_js as js>
	<script type="text/javascript" src="${path}${js}"></script>
	</#list>

</#macro>